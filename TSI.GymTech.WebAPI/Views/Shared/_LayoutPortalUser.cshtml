<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    
    <!-- Custom fonts for this template-->
    <link href="@Url.Content("~/Content/css/sb-admin-2.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/toastr.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/vendor/datatables.net/css/jquery.dataTables.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/vendor/datatables/dataTables.responsive.min.css")" rel="stylesheet" />

    <link href="@Url.Content("~/Content/vendor/fontawesome-free/css/all.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    @RenderSection("AddToHeadAnotherWay", required: false)

</head>
<body id="page-top">
    <!-- Custom styles for this template-->
    <!-- Bootstrap core JavaScript-->
    <script src="@Url.Content("~/Scripts/Utility/UtilityHelper.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/jquery/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/bootstrap/js/bootstrap.bundle.min.js")" type="text/javascript"></script>

    <!-- Core plugin JavaScript-->
    <script src="@Url.Content("~/Content/vendor/jquery-easing/jquery.easing.min.js")" type="text/javascript"></script>

    <!-- Custom scripts for all pages-->
    <script src="@Url.Content("~/Scripts/toastr.js")" type="text/javascript"></script>

    @*CSS and JS files to DataTable componet*@
    <script src="@Url.Content("~/Content/vendor/datatables/jquery.dataTables.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/datatables/dataTables.bootstrap4.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/datatables.net-buttons/js/dataTables.buttons.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/datatables.net-buttons-bs4/js/buttons.bootstrap4.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/datatables/dataTables.responsive.min.js")"></script>
    <script src="@Url.Content("~/Content/vendor/jszip/dist/jszip.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/pdfmake/build/pdfmake.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/pdfmake/build/vfs_fonts.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/datatables.net-buttons/js/buttons.html5.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/datatables.net-buttons/js/buttons.print.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/vendor/datatables.net-buttons/js/buttons.colVis.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Utility/DataTablesUtility.js")" type="text/javascript"></script>

    <!-- Page Wrapper -->
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav bg-dark sidebar sidebar-dark accordion" id="accordionSidebar">
            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="#">
                <div class="sidebar-brand-icon rotate-n-15">
                    <i class="fas fa-laugh-wink"></i>
                </div>
                <div class="sidebar-brand-text mx-3">GymTech <sup>v1.0</sup></div>
            </a>
            <!-- Divider -->
            <hr class="sidebar-divider my-0">
            <li class="nav-item">
                <a class="nav-link" href=@Url.Action("Index", "PortalUser")>
                    <i class="fas fa-users"></i>
                    <span>Portal do Aluno</span>
                </a>
            </li>
            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">

            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>
        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">
            <!-- Main Content -->
            <div id="content">
                @using (Html.BeginForm())
                {
                    <!-- Topbar -->
                    <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
                        <!-- Sidebar Toggle (Topbar) -->
                        <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3" onclick="return false;"
                                data-toggle="collapse" data-target="#navbarSite">
                            <i class="fa fa-bars"></i>
                        </button>
                    </nav>
                }
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid">
                    @RenderBody()
                    <hr />
                </div>
                <!-- /.container-fluid -->
            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; TSI - GymTech - 2019</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->
        </div>
        <!-- End of Content Wrapper -->
    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
    <!-- Custom scripts for all pages-->
    <script src="@Url.Content("~/Scripts/Utility/sb-admin-2.js")" type="text/javascript"></script>
</body>
</html>